{
  "name": "@qomhmd/universal-orm",
  "version": "1.0.0",
  "description": "Universal ORM for multiple databases - Initial code from https://www.npmjs.com/package/universal-orm",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "require": "./dist/adapters/index.js",
      "import": "./dist/adapters/index.js"
    }
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "orm",
    "database",
    "postgres",
    "mongodb",
    "redis",
    "elasticsearch",
    "neo4j",
    "cassandra",
    "dynamodb",
    "influxdb",
    "arangodb",
    "orientdb"
  ],
  "author": "Mohammad Qodrati",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.484.0",
    "@elastic/elasticsearch": "^8.11.0",
    "@influxdata/influxdb-client": "^1.33.2",
    "arangojs": "^8.6.0",
    "cassandra-driver": "^4.7.2",
    "ioredis": "^5.3.2",
    "mongodb": "^6.3.0",
    "neo4j-driver": "^5.15.0",
    "orientjs": "^3.0.11",
    "pg": "^8.11.3",
    "redis": "^4.6.11"
  },
  "devDependencies": {
    "@types/ioredis": "^4.28.10",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^24.4.0",
    "@types/pg": "^8.15.5",
    "@types/redis": "^4.0.10",
    "@types/sequelize": "^4.28.20",
    "jest": "^29.7.0",
    "typescript": "^5.9.2"
  }
}
